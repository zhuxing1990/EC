<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.view.ViewPager
        android:id="@+id/walkintoyiyang_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:paddingLeft="268dp"
        android:paddingRight="268dp"
        android:background="@color/color_blue2">
            <RelativeLayout
                android:id="@+id/walkintoyiyang_overview_rl"
                android:layout_width="186dp"
                android:layout_height="90dp"
                android:background="@drawable/layout_selector_relative">
                <TextView
                    android:id="@+id/walkintoyiyang_text1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="30sp"
                    android:textColor="@color/color_white"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/walkintoyiyang_historical_culture_rl"
                android:layout_toRightOf="@+id/walkintoyiyang_overview_rl"
                android:layout_width="186dp"
                android:layout_height="90dp"
                android:background="@drawable/layout_selector_relative">
                <TextView
                    android:id="@+id/walkintoyiyang_text2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="30sp"
                    android:textColor="@color/color_white"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/walkintoyiyang_todays_style_rl"
                android:layout_toRightOf="@+id/walkintoyiyang_historical_culture_rl"
                android:layout_width="186dp"
                android:layout_height="90dp"
                android:background="@drawable/layout_selector_relative">
                <TextView
                    android:id="@+id/walkintoyiyang_text3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="30sp"
                    android:textColor="@color/color_white"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/walkintoyiyang_future_development_rl"
                android:layout_toRightOf="@+id/walkintoyiyang_todays_style_rl"
                android:layout_width="186dp"
                android:layout_height="90dp"
                android:background="@drawable/layout_selector_relative">
                <TextView
                    android:id="@+id/walkintoyiyang_text4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="30sp"
                    android:textColor="@color/color_white"/>
            </RelativeLayout>
        <com.vunke.ec.view.FlyBorderView
            android:id="@+id/walkintoyiyang_flyborderview"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:visibility="invisible"
            android:background="@drawable/bg_border1"/>
    </RelativeLayout>
</RelativeLayout>